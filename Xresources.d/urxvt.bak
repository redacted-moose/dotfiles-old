! URxvt resources
! Global
URxvt*.color0: #000000
URxvt*.color1: #CC0000
URxvt*.color2: #4E9A06
URxvt*.color3: #C4A000
URxvt*.color4: #3465A4
URxvt*.color5: #75507B
URxvt*.color6: #06989A
URxvt*.color7: #D3D7CF

URxvt*.color8: #555753
URxvt*.color9: #EF2929
URxvt*.color10: #8AE234
URxvt*.color11: #FCE94F
URxvt*.color12: #729FCF
URxvt*.color13: #AD7FA8
URxvt*.color14: #34E2E2
URxvt*.color15: #EEEEEC
! URxvt.url-select.launcher:  chromium
! URxvt.url-select.underline: true
! URxvt.keysym.M-u:           perl:url-select:select_next
! URxvt.keysym.M-Escape:      perl:keyboard-select:activate
! URxvt.keysym.M-s:           perl:keyboard-select:search

! Terminal window
urxvt*.font: xft:Terminess\ Powerline:size=14:pixelsize=14,xft:Inconsolata\ for\ Powerline:size=14:pixelsize=14
urxvt*.boldFont: xft:Terminess\ Powerline:bold:size=14:pixelsize=14,xft:Inconsolata\ for\ Powerline:bold:size=14:pixelsize=14
urxvt*.letterSpace: 0
urxvt*.transparent: false
urxvt*.scrollBar: false
urxvt*.reverseVideo: true
urxvt*.perl-ext-common: default,clipboard,selection-to-clipboard
urxvt*.iso14755: false
urxvt*.keysym.C-C: perl:clipboard:copy
urxvt*.keysym.C-V: perl:clipboard:paste
urxvt*.keysym.C-A-V: perl:clipboard:paste_escaped
urxvt*.cursorBlink: true

! urxvt*.clipboard.copycmd: xsel -ib
! urxvt*.clipboard.pastecmd: xsel -ob

! Quake window
urxvtq*.scrollBar: false
urxvtq*.font: xft:Terminess\ Powerline:size=14:pixelsize=14,xft:Inconsolata\ for\ Powerline:size=14:pixelsize=14
urxvtq*.boldFont: xft:Terminess\ Powerline:bold:size=14:pixelsize=14,xft:Inconsolata\ for\ Powerline:bold:size=14:pixelsize=14
urxvtq*.perl-ext-common: default,clipboard,selection-to-clipboard
urxvtq*.reverseVideo: false
urxvtq*.depth: 32
urxvtq*.background: [75]#000000
urxvtq*.foreground: [100]#FFFFFF
urxvtq*.cursorBlink: true
urxvtq*.iso14755: false
urxvtq*.keysym.C-C: perl:clipboard:copy
urxvtq*.keysym.C-V: perl:clipboard:paste
urxvtq*.keysym.C-A-V: perl:clipboard:paste_escaped
